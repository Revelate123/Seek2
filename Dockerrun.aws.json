{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "app",
        "image": "revelate123/app-image:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 3000,
            "containerPort": 3000
          }
        ],
        "links": [
          "flask"
        ]
      },
      {
        "name": "flask",
        "image": "revelate123/flask-image:latest",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 5000,
            "containerPort": 5000
          }
        ]
      },
      {
        "name": "mongodb",
        "image": "mongo:6-jammy",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 27017,
            "containerPort": 27017
          }
        ],
        "mountPoints": [
          {
            "sourceVolume": "dbdata6",
            "containerPath": "/data/db"
          }
        ]
      }
    ],
    "volumes": [
      {
        "name": "dbdata6",
        "host": {
          "sourcePath": "/var/app/dbdata6"
        }
      }
    ]
  }
  